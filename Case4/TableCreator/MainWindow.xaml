<Window x:Class="TableCreator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TableCreator"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="300">
    <Grid>
        <DockPanel LastChildFill="False">
            <Button x:Name="B_CreateTable" DockPanel.Dock="Top" Content="Create Table" Height="40" Margin="0,5,0,0"/>
            <Button x:Name="B_CreateFields" DockPanel.Dock="Top" Content="Create Fields" Height="40" Margin="0,5,0,0"/>
        </DockPanel>
    </Grid>
</Window>
